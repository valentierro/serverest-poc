{
  "name": "cypress-poc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:edge": "cypress run --browser edge",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:api": "cypress run --spec 'cypress/e2e/api/**/*'",
    "test:api:headed": "cypress run --headed --spec 'cypress/e2e/api/**/*'",
    "test:api:usuarios": "cypress run --spec 'cypress/e2e/api/usuarios.spec.ts'",
    "test:api:produtos": "cypress run --spec 'cypress/e2e/api/produtos.spec.ts'",
    "test:api:carrinho": "cypress run --spec 'cypress/e2e/api/carrinho.spec.ts'",
    "test:busca-lista": "cypress run --spec 'cypress/e2e/ui/busca-lista-produtos.spec.ts'",
    "test:busca-lista:headed": "cypress run --headed --spec 'cypress/e2e/ui/busca-lista-produtos.spec.ts'",
    "test:cadastro": "cypress run --spec 'cypress/e2e/ui/cadastro-usuario.spec.ts'",
    "test:cadastro:headed": "cypress run --headed --spec 'cypress/e2e/ui/cadastro-usuario.spec.ts'",
    "test:login": "cypress run --spec 'cypress/e2e/ui/login-usuario.spec.ts'",
    "test:login:headed": "cypress run --headed --spec 'cypress/e2e/ui/login-usuario.spec.ts'",
    "test:todos": "cypress run --spec 'cypress/e2e/ui/**/*,cypress/e2e/api/**/*'",
    "test:todos:headed": "cypress run --headed --spec 'cypress/e2e/ui/**/*,cypress/e2e/api/**/*'",
    "test:dev": "CYPRESS_ENV=dev cypress run",
    "test:stg": "CYPRESS_ENV=stg cypress run",
    "test:prod": "CYPRESS_ENV=prod cypress run",
    "test:dev:headed": "CYPRESS_ENV=dev cypress run --headed",
    "test:stg:headed": "CYPRESS_ENV=stg cypress run --headed",
    "test:prod:headed": "CYPRESS_ENV=prod cypress run --headed",
    "test:api:dev": "CYPRESS_ENV=dev cypress run --spec 'cypress/e2e/api/**/*'",
    "test:api:stg": "CYPRESS_ENV=stg cypress run --spec 'cypress/e2e/api/**/*'",
    "test:api:prod": "CYPRESS_ENV=prod cypress run --spec 'cypress/e2e/api/**/*'",
    "test:ui:dev": "CYPRESS_ENV=dev cypress run --spec 'cypress/e2e/ui/**/*'",
    "test:ui:stg": "CYPRESS_ENV=stg cypress run --spec 'cypress/e2e/ui/**/*'",
    "test:ui:prod": "CYPRESS_ENV=prod cypress run --spec 'cypress/e2e/ui/**/*'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.9.2",
    "cypress": "^15.5.0",
    "typescript": "^5.9.3"
  }
}
